@{
    ViewData["Title"] = "PMU Frequency";
}

<div class="row plotContainer">
    <div id="plotDiv" class="col-md-12"></div>
</div>
<div class="row">
    <label class="mr-3">Fetch for last </label>
    <input value="10" id="minsOffsetInp" placeholder="Enter Offset minutes" style="width:3em" />
    <label class="mr-3">minutes</label>
    <button class="btn btn-sm btn-outline-success" id="refreshBtn">Refresh Data</button>
</div>
<div class="row">
    <button class="btn btn-sm btn-outline-info" id="plotExportBtn">Export Data</button>
</div>

@section Scripts {
    @*<script src="https://cdn.plot.ly/plotly-latest.min.js"></script>*@
    <script src="~/js/fict-dash-app/node_modules/plotly.js-dist/plotly.js"></script>
    <script src="~/js/fict-dash-app/dist/pmu_freq.js"></script>
    <script src="~/js/fict-dash-app/node_modules/chosen-js/chosen.jquery.min.js"></script>
}